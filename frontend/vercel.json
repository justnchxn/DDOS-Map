{
  "builds": [
    { "src": "vite.config.js", "use": "@vercel/static-build", "config": { "distDir": "dist" } },
    { "src": "api/events.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/dist/$1" }
  ],
  "devCommand": "vite --port $PORT",
  "builds": [
    { "src": "vite.config.js", "use": "@vercel/static-build", "config": { "distDir": "dist" } },
    { "src": "api/events.js", "use": "@vercel/node" }
  ]
}
